<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Servlet0</servlet-name>
        <servlet-class>servlets.Servlet0</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo01Servlet</servlet-name>
        <servlet-class>servlets.Demo01</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet0</servlet-name>
        <url-pattern>/Servlet0</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>demo01Servlet</servlet-name>
        <url-pattern>/demo01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>session0</servlet-name>
        <servlet-class>servlets.session_0</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>session0</servlet-name>
        <url-pattern>/session0</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>session1</servlet-name>
        <servlet-class>servlets.session_1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>session1</servlet-name>
        <url-pattern>/session1</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>forward0</servlet-name>
        <servlet-class>servlets.forward_0</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>forward0</servlet-name>
        <url-pattern>/forward0</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>forward1</servlet-name>
        <servlet-class>servlets.forward_1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>forward1</servlet-name>
        <url-pattern>/forward1</url-pattern>
    </servlet-mapping>














</web-app>