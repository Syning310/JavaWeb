        <servlet-name>applicat01</servlet-name>
        <servlet-class>com.demo.Application01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>applicat01</servlet-name>
        <url-pattern>/app01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>applicat02</servlet-name>
        <servlet-class>com.demo.Application02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>applicat02</servlet-name>
        <url-pattern>/app02</url-pattern>
    </servlet-mapping>
    
    
</web-app>