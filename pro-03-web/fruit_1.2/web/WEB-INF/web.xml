<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
        <servlet>
            <servlet-name>fruit1.1</servlet-name>
            <servlet-class>com.servlets.IndexServlet</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>fruit1.1</servlet-name>
            <url-pattern>/abb</url-pattern>
        </servlet-mapping>
    
    <context-param>
        <param-name>view-prefix</param-name>
        <param-value>/</param-value>
    </context-param>
    
    <context-param>
        <param-name>view-suffix</param-name>
        <param-value>.html</param-value>
    </context-param>
    
    <servlet>
        <servlet-name>req01</servlet-name>
        <servlet-class>com.demo.Request01Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>req01</servlet-name>
        <url-pattern>/req01</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>req02</servlet-name>
        <servlet-class>com.demo.Request02Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>req02</servlet-name>
        <url-pattern>/req02</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ssion01</servlet-name>
        <servlet-class>com.demo.Sess01Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ssion01</servlet-name>
        <url-pattern>/ssion01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ssion02</servlet-name>
